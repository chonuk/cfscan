#!/usr/bin/env python

import sys
from cftester import CFTester


if __name__ == "__main__":

    if len(sys.argv) != 2:
        sys.stderr.write("syntax: %s <cf_api_target>\n" % sys.argv[0])
        exit(1)

    tester = CFTester(sys.argv[1], sys.stdout, sys.stderr)
    tester.run_tests()
    exit(0)
